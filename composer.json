{
    "name": "duyler/di",
    "type": "library",
    "description": "Dependency injection container",
    "license": "MIT",
    "keywords": [
        "di",
        "dependency-injection"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "require": {
        "php": ">=8.3",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.30",
        "vimeo/psalm": "^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Duyler\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\DI\\Test\\": ["tests/"]
        }
    }
}
